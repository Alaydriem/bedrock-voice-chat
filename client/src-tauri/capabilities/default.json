{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "deep-link:default",
    "store:default",
    "os:default",
    "log:default",
    "stronghold:default",
    {
      "local": true,
      "windows": ["*"],
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://**/*"
        },
        {
          "url": "https://**"
        },
        {
          "url": "https://*:*"
        }
      ]
    }
  ]
}
